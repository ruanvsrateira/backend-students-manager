// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id          Int       @id   @default(autoincrement())
  name        String  
  email       String    @unique
  age         Int
  cpf         String
  created_at  DateTime  @default(now())

  @@map("students")
}